{
  "name": "http-server",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "scripts": {
    "build": "tsc -p tsconfig.src.json",
    "check": "tsc -b tsconfig.json",
    "dev": "tsx --env-file=.env --watch src/main.ts",
    "dev-client": "tsx --env-file=.env src/client.ts",
    "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
    "lint-fix": "pnpm lint --fix",
    "test": "vitest",
    "test:unit": "vitest run test/unit",
    "test:integration": "./test/integration/server-lifecycle.sh test",
    "test:integration-only": "vitest run test/integration/client-tests",
    "test:integration:aws": "vitest run test/integration/aws-api.test.ts",
    "test:integration:complete": "./test/scripts/integration-test.sh",
    "test:integration:local": "./test/scripts/integration-test.sh --local-only",
    "test:integration:aws-only": "./test/scripts/integration-test.sh --aws-only",
    "test:integration:compare": "./test/scripts/integration-test.sh --compare-only",
    "test:coverage": "vitest run test/unit --coverage",
    "test:coverage:all": "vitest run --coverage",
    "test:docker": "./test/docker/docker-integration.sh test",
    "test:docker:build": "./test/docker/docker-integration.sh build",
    "test:docker:start": "./test/docker/docker-integration.sh start",
    "test:docker:clean": "./test/docker/docker-integration.sh clean",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:health": "./test/scripts/health-check.sh",
    "test:all": "./test/scripts/run-all-tests.sh",
    "test:all:quick": "./test/scripts/run-all-tests.sh --quick",
    "openapi:generate": "tsx scripts/generate-openapi.ts",
    "docker:build": "docker build -t effect-app .",
    "docker:run": "docker run -p 3000:3000 effect-app",
    "aws:deploy": "./scripts/deploy.sh",
    "aws:destroy": "./scripts/deploy.sh destroy",
    "aws:test": "./test/scripts/deployment-test.sh",
    "aws:test:curl": "./test/scripts/api-test.sh",
    "aws:test:load": "./test/scripts/api-test.sh --load-test",
    "aws:logs": "aws logs tail /ecs/effect-app-fargate --follow --region us-west-2",
    "aws:status": "./test/scripts/deployment-test.sh --quick",
    "aws:monitor": "./monitor-aws.sh check",
    "aws:monitor:continuous": "./monitor-aws.sh monitor",
    "aws:alerts": "./monitor-aws.sh alerts"
  },
  "keywords": [],
  "author": "",
  "packageManager": "pnpm@9.10.0",
  "devDependencies": {
    "@effect/language-service": "^0.2.0",
    "@effect/vitest": "^0.25.0",
    "@types/node": "^22.8.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@effect/opentelemetry": "^0.39.7",
    "@effect/platform": "^0.69.12",
    "@effect/platform-node": "^0.64.13",
    "@effect/sql": "^0.18.13",
    "@effect/sql-sqlite-node": "^0.19.2",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.10.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.54.0",
    "@opentelemetry/sdk-trace-base": "^1.27.0",
    "@opentelemetry/sdk-trace-node": "^1.27.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "aws-cdk-lib": "^2.208.0",
    "constructs": "^10.4.2",
    "effect": "^3.10.7",
    "eslint": "^9.10.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-codegen": "^0.28.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "uuid": "^11.0.2"
  }
}
